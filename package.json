    "scripts": {
        "start": "set REACT_APP_MODE=app & react-scripts start",
        "start:tv": "set REACT_APP_MODE=tv & react-scripts start",
        "build": "node ./increment_build.js && set REACT_APP_MODE=app && react-scripts build && node ./modify-sw.js",
        "build:tv": "node ./increment_build.js && set REACT_APP_MODE=tv&& react-scripts build && node ./modify-sw.js",
        "deploy": " scp -r build/* u45065@www.wolkenberg-gymnasium.de:~/wolkenberg-app/stundenplan-web-app",
        "deploy:tv": " scp -r build/* u45065@www.wolkenberg-gymnasium.de:~/wolkenberg-app/stundenplan-web-app-tv",
        "push": "npm run build && npm run deploy",
        "push:tv": "npm run build:tv && npm run deploy:tv",
        "test": "react-scripts test --env=jsdom",
        "eject": "react-scripts eject",
        "lint": "eslint src/ --cache --fix && echo \"eslint: no lint errors\"",
        "prettier": "prettier --write ./src/**/*.js"
    },
    "devDependencies": {
        "nodegit": "^0.24.1",
        "redux-devtools-extension": "^2.13.8"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ]
}
